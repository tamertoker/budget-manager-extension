{
	"manifest_version": 3,
	"name": "Budget Manager",
	"version": "1.0",
	"description": "This extension tracks your overall spendings!",
	"icons": {
		"128": "icon128.png",
		"48": "icon48.png",
		"16": "icon16.png"
	},
	"action":{
		"default": "icon16.png",
		"default_popup": "popup.html"
	},

	"options_page" : "options.html",

	"background": {
		"service_worker" : "eventPage.js"
	},

	"permissions": [
		"storage",
		"notifications",
		"contextMenus"
			]

}